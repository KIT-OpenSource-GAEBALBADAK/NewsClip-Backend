# PostgreSQL 연결 정보
DB_HOST=localhost
DB_USER=your_db_user
DB_PASSWORD= # 실제 비밀번호는 비워두세요
DB_NAME=newsclip_db
DB_PORT=5432
DB_SSLMODE=disable
DB_TIMEZONE=Asia/Seoul

# JWT 시크릿 키 (다른 개발자가 직접 생성하도록 안내)
JWT_SECRET_KEY= # 여기에 강력한 키를 직접 생성하여 입력하세요

# .env 파일 맨 아래에 추가
KAKAO_REST_API_KEY=여기에_복사한_REST_API_키를_붙여넣으세요